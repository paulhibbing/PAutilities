citHeader("To cite package 'PAutilities' in publications use:")

citEntry(entry = "Misc",
  title        = "PAutilities: Utility functions for physical activity research",
  author       = personList(as.person("Paul R. Hibbing")),
  year         = "2019",
  url          = "https://github.com/paulhibbing/PAutilities/",

  textVersion  =
  paste("Paul R. Hibbing (2019).",
        "PAutilities: Utility functions for physical activity research.",
        "URL https://github.com/paulhibbing/PAutilities/.")
)
